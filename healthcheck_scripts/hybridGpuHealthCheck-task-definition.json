{
    "family": "hybridGpuHealthCheckV1",
    "containerDefinitions": [
        {
            "name": "healthcheck",
            "image": "455385591292.dkr.ecr.cn-northwest-1.amazonaws.com.cn/hybridgpu:healthcheck",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 2022,
                    "hostPort": 2022,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [
                "/bin/sh"
            ],
            "command": [
                "/healthcheck/healthCheckMaster.sh"
            ],
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "lustre",
                    "containerPath": "/healthcheck",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "linuxParameters": {
                "devices": [
                    {
                        "hostPath": "/dev/infiniband",
                        "containerPath": "/dev/infiniband",
                        "permissions": [
                            "read",
                            "write"
                        ]
                    }
                ]
            },
            "privileged": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/hybridGpuHealthcheck",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "cn-northwest-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": [],
            "resourceRequirements": [
                {
                    "value": "8",
                    "type": "GPU"
                }
            ]
        }
    ],
    "taskRoleArn": "arn:aws-cn:iam::455385591292:role/ecsanywhereTaskRole",
    "executionRoleArn": "arn:aws-cn:iam::455385591292:role/ecsanywhereTaskExecutionRole",
    "networkMode": "host",
    "volumes": [
        {
            "name": "lustre",
            "host": {
                "sourcePath": "/fsx"
            }
        }
    ],
    "placementConstraints": [
        {
            "type": "memberOf",
            "expression": "attribute:node==node002"
        }
    ],
    "requiresCompatibilities": [
        "EXTERNAL"
    ],
    "cpu": "4096",
    "memory": "32768",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}